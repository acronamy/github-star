{
  "name": "github-star-server",
  "version": "1.0.0",
  "description": "Node server with github star service to provice for the React app",
  "main": "index.js",
  "config": {
    "server": {
      "hostname": "localhost",
      "protocol": "http",
      "port": "3030"
    }
  },
  "scripts": {
    "open-port": "kill-port --port $npm_package_config_server_port",
    "serve": "npm run open-port && node index.js",
    "test": "npm run serve & npx jest"
  },
  "author": "Adam Crockett",
  "license": "ISC",
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "kill-port": "^1.5.1"
  }
}
